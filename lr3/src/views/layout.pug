include components/navbar

doctype html
html(lang="ru")
    head
        meta(charset="UTF-8")
        meta(name="viewport" content="width=device-width, initial-scale=1.0")
        block head
        if activeTitle
            title= activeTitle
        if activeLink
            script(src=`/webpack-build/${activeLink}.bundle.js` defer)
        link(rel="icon" href="/static/assets/favicon.svg" type="image/svg+xml")
    body
        +navbar(navbarOptions)
        main
            block content

